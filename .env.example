# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_APP_ID=your_discord_app_id_here
DISCORD_GUILD_ID=your_discord_guild_id_here
DISCORD_COMMANDS_CHANNEL_ID=your_commands_channel_id_here
DISCORD_UPDATES_CHANNEL_ID=your_updates_channel_id_here

# Optional Discord Webhooks for agent personas
COMMUNICATIONS_WEBHOOK_URL=
PM_WEBHOOK_URL=
SD_WEBHOOK_URL=
JD_WEBHOOK_URL=
RQE_WEBHOOK_URL=

# Obsidian Vault Integration
OBSIDIAN_VAULT_PATH=/path/to/your/obsidian/vault

# Provider API Keys
DEEPSEEK_API_KEY=your_deepseek_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here
QWEN_API_KEY=your_qwen_api_key_here

# Model Usage Settings
# Set to false to restrict routing to free models/routes where available
USE_PAID_MODELS=false
# Optionally specify allowed tiers explicitly (overrides USE_PAID_MODELS):
# free, cheap, budget, performance, ultra (comma-separated)
# ALLOWED_MODEL_TIERS=free,cheap,budget

# Development Settings
LOG_LEVEL=INFO

# Discord privileged intents (default OFF unless enabled in Dev Portal)
DISCORD_MESSAGE_CONTENT=0
DISCORD_MEMBERS=0
DISCORD_PRESENCE=0
